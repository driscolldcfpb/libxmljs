{
  "name": "libxmljs",
  "author": "Marco Rogers",
  "contributors": [
    "Jeff Smick"
  ],
  "binary": {
    "module_name": "xmljs",
    "module_path": "./build/Release/",
    "host": "https://github.com",
    "remote_path": "./libxmljs/libxmljs/releases/download/v{version}/",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz"
  },
  "description": "libxml bindings for v8 javascript engine",
  "version": "0.18.8",
  "scripts": {
    "install": "node-gyp rebuild --verbose",
    "test": "node --expose_gc ./node_modules/.bin/nodeunit test"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/libxmljs/libxmljs.git"
  },
  "bugs": {
    "url": "http://github.com/libxmljs/libxmljs/issues"
  },
  "main": "./index",
  "license": "MIT",
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "~1.0.0",
    "bindings": "1.2.1",
    "nan": "~2.5.0"
  },
  "devDependencies": {
    "nodeunit": "0.9.0"
  }
}
